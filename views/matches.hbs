{{> header_matches}}

<main id="main" role="main">
    <section id="live" class="matches__items">
        {{#items}}
            {{#if liveCMD}}
                <div class="matches__division">
                    <h1 class="matches__division__title">CMD Amsterdam</h1>
                    <div class="matches__container">
                        {{#each liveCMD}}
                            {{> content_matches}}
                        {{/each}}
                    </div>
                </div>
            {{/if}}
            {{#if liveWomen}}
                <div class="matches__division">
                    <h1 class="matches__division__title">Women</h1>
                    <div class="matches__container">
                        {{#each liveWomen}}
                            {{> content_matches}}
                        {{/each}}
                    </div>
                </div>
            {{/if}}
            {{#if liveOpen}}
                <div class="matches__division">
                    <h1 class="matches__division__title">Open</h1>
                    <div class="matches__container">
                        {{#each liveOpen}}
                            {{> content_matches}}
                        {{/each}}
                    </div>
                </div>
            {{/if}}
            {{#if liveMixed}}
                <div class="matches__division">
                    <h1 class="matches__division__title">Mixed</h1>
                    <div class="matches__container">
                    {{#each liveMixed}}
                        {{> content_matches}}
                    {{/each}}
                    </div>
                </div>
            {{/if}}
        {{/items}}
    </section>
    <section id="recent" class="matches__items">
        {{#items}}
            {{#if recentCMD}}
                <div class="matches__division">
                    <h1 class="matches__division__title">CMD Amsterdam</h1>
