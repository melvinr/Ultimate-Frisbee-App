                    <h1 class="matches__division__title">Mixed</h1>
                    <div class="matches__container">
                    {{#each upcomingMixed}}
                        {{> content_matches}}
                    {{/each}}
                    </div>
                </div>
            {{/if}}
        {{/items}}
    </section>
</main>

{{> footer_matches}}
