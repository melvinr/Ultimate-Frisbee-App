                    <h1 class="matches__division__title">Women</h1>
                    <div class="matches__container">
                    {{#each recentWomen}}
                        {{> content_matches}}
                    {{/each}}
                    </div>
                </div>
            {{/if}}
            {{#if recentOpen}}
                <div class="matches__division">
                    <h1 class="matches__division__title">Open</h1>
                    <div class="matches__container">
                    {{#each recentOpen}}
                        {{> content_matches}}
                    {{/each}}
                </div>
                </div>
            {{/if}}
            {{#if recentMixed}}
                <div class="matches__division">
                    <h1 class="matches__division__title">Mixed</h1>
                    <div class="matches__container">
                    {{#each recentMixed}}
                        {{> content_matches}}
                    {{/each}}
                    </div>
                </div>
            {{/if}}
        {{/items}}
    </section>
    <section id="upcoming" class="matches__items">
        {{#items}}
            {{#if upcomingCMD}}
                <div class="matches__division">
                    <h1 class="matches__division__title">CMD Amsterdam</h1>
                    <div class="matches__container">
                    {{#each upcomingCMD}}
                        {{> content_matches}}
                    {{/each}}
                    </div>
                </div>
            {{/if}}
            {{#if upcomingWomen}}
                <div class="matches__division">
                    <h1 class="matches__division__title">Women</h1>
                    <div class="matches__container">
                    {{#each upcomingWomen}}
                        {{> content_matches}}
                    {{/each}}
                    </div>
                </div>
            {{/if}}
            {{#if upcomingOpen}}
                <div class="matches__division">
                    <h1 class="matches__division__title">Open</h1>
                    <div class="matches__container">
                    {{#each upcomingOpen}}
                        {{> content_matches}}
                    {{/each}}
                    </div>
                </div>
            {{/if}}
            {{#if upcomingMixed}}
